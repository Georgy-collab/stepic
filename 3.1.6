import java.awt.Color;
import java.awt.Graphics;
import javax.swing.JFrame;
import javax.swing.JPanel;

public class Main {

    public static void main(String[] args) {
        JFrame frame = new JFrame("Drawing Example");
        frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
        frame.setSize(800, 600);

        JPanel panel = new JPanel() {
            @Override
            public void paintComponent(Graphics g) {
                super.paintComponent(g);
                g.setColor(Color.BLACK);
                g.drawOval(300, 300, 200, 200);
                g.setColor(Color.WHITE);
                g.fillOval(301, 301, 198, 198);

                g.setColor(Color.BLACK);
                g.drawOval(325, 200, 150, 150);
                g.setColor(Color.WHITE);
                g.fillOval(326, 201, 148, 148);

                g.setColor(Color.BLACK);
                g.drawOval(350, 125, 100, 100);
                g.setColor(Color.WHITE);
                g.fillOval(351, 126, 98, 98);

                g.setColor(Color.BLACK);
                g.drawOval(400, 240, 15, 15);
                g.drawOval(400, 270, 15, 15);
                g.drawOval(400, 300, 15, 15);

                g.drawOval(350, 115, 100, 20);
                g.setColor(Color.WHITE);
                g.fillOval(351, 116, 98, 18);

                g.setColor(Color.BLACK);
                g.drawRoundRect(375, 68, 50, 50, 5, 5);
                g.setColor(Color.WHITE);
                g.fillRoundRect(376, 69, 48, 48, 5, 5);
                g.setColor(Color.BLACK);
                g.drawLine(375, 100, 425, 100);

                g.drawOval(400, 160, 15, 15);
                g.drawOval(430, 160, 15, 15);
            }
        };

        frame.add(panel);
        frame.setVisible(true);
    }
}
